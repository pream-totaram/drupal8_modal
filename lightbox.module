<?php

function lightbox_page_attachments(&$vars) {
    if(\Drupal::service('current_route_match')->getRouteName() === 'view.home.page_1') {
        $vars['#attached']['library'][] = 'lightbox/colorbox';
        $vars['#attached']['library'][] = 'lightbox/lightbox';
    }
}

function lightbox_theme($existing, $type, $theme, $path) {
    return [
        'output' => [
            'variables' => ['var' => null]
        ]
    ];
}